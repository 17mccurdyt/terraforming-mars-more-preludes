{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["dom", "ES6"], 
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outDir": "dist",
    "sourceMap": true,
    "target": "es6",
    "types": ["node"]
  },
    "files": [
        "script.ts",
	    "server.ts",
        "src/Game.ts",
        "src/PlayerRoute.ts"
    ]
}
